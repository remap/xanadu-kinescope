{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "default": {
      "format": "[%(asctime)s] %(name)s: %(levelname)s: %(message)s"
    },
    "color": {
      "()": "__main__.ColorFormatter",
      "format": "[%(asctime)s] %(name)s: %(levelname)s: %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "color",
      "level": "DEBUG"
    },
    "file": {
      "class": "logging.FileHandler",
      "filename": "Oracle.log",
      "formatter": "default",
      "level": "DEBUG"
    },
    "websocket": {
      "class": "__main__.WebSocketLogHandler",
      "formatter": "color",
      "level": "DEBUG"
    }
  },
  "root": {
    "handlers": [
      "console",
      "file",
      "websocket"
    ],
    "level": "INFO"
  }
}
